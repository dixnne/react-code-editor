fn fibonacci(n: Int) -> Int {
	if (n <= 1) {
		return n;
	}
	return fibonacci(n - 1) + fibonacci(n - 2);
}

fn main() -> Int {
	let n: Int = 0;
	printf("Enter how many Fibonacci numbers to calculate: ");
	scanf("%d", n);
	
	let i: Int = 0;
	while (i < n) {
		printf("fibonacci(%d) = %d\n", i, fibonacci(i));
		i = i + 1;
	}
	
	return 0;
}
